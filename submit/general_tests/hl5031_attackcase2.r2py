try:
    try:
        f = openfile("racefile", True)
    except Exception:
        f = None

    if f:
        try:
            f.writeat("X", 0)
        except Exception:
            pass
        try:
            try:
                sleep(0.05)
            except Exception:
                pass
            f.close()
        except Exception:
            pass

    try:
        _ = listfiles()
    except Exception:
        pass
except Exception:
    pass
