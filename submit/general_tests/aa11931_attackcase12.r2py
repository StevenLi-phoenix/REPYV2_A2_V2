# Test: Multiple versions with correct content

fmulti = openfile("multiversion12", True)
fmulti.writeat("v0", 0)
fmulti.close()

for i in range(1, 5):
    fm = openfile("multiversion12", True)
    fm.writeat("v" + str(i), 0)
    fm.close()

# Verify all versions exist and have correct content
fbase = openfile("multiversion12", False)
assert fbase.readat(None, 0) == "v0"
fbase.close()

for i in range(1, 5):
    fv = openfile("multiversion12.v" + str(i), False)
    assert fv.readat(None, 0) == "v" + str(i), "Version should have correct content"
    fv.close()
