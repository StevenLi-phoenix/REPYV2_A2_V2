try:
    f = openfile("deepchain.txt", True)
    f.writeat("v0", 0)
    f.close()
    
    for i in range(30):
        try:
            v = openfile("deepchain.txt", True)
            v.writeat("version" + str(i), 0)
            v.close()
        except Exception as e:
            pass
    
    for i in range(1, 31):
        try:
            v = openfile("deepchain.txt.v" + str(i), False)
            content = v.readat(None, 0)
            try:
                v.writeat("exploit", 0)
            except FileInUseError:
                pass
            except Exception as e:
                pass
            v.close()
        except FileNotFoundError:
            pass
        except Exception as e:
            pass
    
    try:
        current = openfile("deepchain.txt", False)
        current.close()
    except Exception as e:
        pass

except Exception as e:
    pass
