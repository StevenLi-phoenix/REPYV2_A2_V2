# pg2820_attackcase2.r2py
# Open the same name twice to simulate concurrent writers.
try:
    a = openfile("data.txt", True)
    b = openfile("data.txt", True)
    try:
        a.writeat("first_writer\n", 0)
    except Exception:
        pass
    try:
        b.writeat("second_writer\n", 0)
    except Exception:
        pass
    try: a.close()
    except Exception: pass
    try: b.close()
    except Exception: pass
except Exception:
    pass
EOF
