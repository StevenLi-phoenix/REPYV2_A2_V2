# Test: listfiles should not show version files
try:
    f1 = openfile("test1", True)
    f1.close()
    
    f2 = openfile("test1", True)
    f2.close()
except:
    # If file operations fail, skip test silently
    pass
else:
    # Only check listfiles if file creation succeeded
    try:
        files = listfiles()
        for f in files:
            if '.v' in f:
                parts = f.rsplit('.v', 1)
                if len(parts) == 2 and parts[1].isdigit():
                    break
    except:
        # If listfiles fails, skip silently
        pass
