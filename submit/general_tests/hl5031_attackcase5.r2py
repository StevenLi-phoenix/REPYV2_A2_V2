try:
    try:
        f = openfile("bigfile", True)
    except Exception:
        f = None

    if f:
        try:
            chunk = "X" * 65536
            for i in range(128):
                try:
                    f.writeat(chunk, i * len(chunk))
                except Exception:
                    pass
        except Exception:
            pass
        try:
            f.close()
        except Exception:
            pass

    try:
        f2 = openfile("bigfile", True)
    except Exception:
        f2 = None

    if f2:
        try:
            _ = f2.readat(None, 0)
        except Exception:
            pass
        try:
            f2.close()
        except Exception:
            pass
except Exception:
    pass
